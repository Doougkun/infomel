{
  "expo": {
    "name": "InfoMel",
    "slug": "infomel",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/bee-logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "br.com.infomel",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/bee-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/bee-logo.png"
    }
  }
}
